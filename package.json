  "scripts": {
    "dev": "NODE_ENV=development nuxt",
    "build": "./scripts/check-is-master.sh && npm run build-json && cd ../ && ./s3-sync-mtbva.sh && cd mountain-bike-virginia && nuxt generate",
    "build-json": "node ./scripts/build-routes-json.js && ./scripts/copy-json.sh && npm run test-j",
    "generate": "nuxt generate",
    "clean": "rimraf .nuxt && rimraf ./dist",
    "start": "NODE_ENV=production node ./server.js",
    "static": "NODE_ENV=production node ./static.js",
    "static-test": "NODE_ENV=test node ./static.js",
    "sleep": "node ./scripts/sleep.js",
    "gen-imgs": "node ./scripts/responsive-imgs.js",
    "lint": "eslint --fix **/*.js *.js **/*.vue  --ignore-pattern node_modules --ignore-pattern dist --ignore-pattern .nuxt",
    "test-one": "JUST_ONE=true mocha --require babel-core/register ./test/integration/global.chrome.spec.js --timeout 30000;",
    "test-i": "mocha --require babel-core/register ./test/integration/global.chrome.spec.js --timeout 30000; mocha --require babel-core/register ./test/integration/global.safari.spec.js --timeout 30000",
    "test-safari": "mocha --require babel-core/register ./test/integration/global.safari.spec.js --timeout 30000",
    "test-u": "mocha --require babel-core/register ./test/unit/global.spec.js --timeout 30000",
    "test-j": "mocha --require babel-core/register ./test/json/global.spec.js --timeout 30000",
    "test-img": "mocha --require babel-core/register ./test/images.spec.js --timeout 30000",
    "test": "mocha --require babel-core/register ./test/global.spec.js --timeout 30000",
    "deploy-prod": "./scripts/check-is-master.sh && NODE_ENV=production npm run build && ./scripts/deploy-prod.sh"
  },
  "main": "dist/app.bundle.js"
}
