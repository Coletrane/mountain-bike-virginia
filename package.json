  "scripts": {
    "dev": "NODE_ENV=development nuxt",
    "build": "npm run sync-json && nuxt generate",
    "sync-json": "rm -r ../mtbva-s3-bucket/posts && rm -r ../mtbva-s3-bucket/authors && rm -r ../mtbva-s3-bucket/results && cp -r ./json/* ../mtbva-s3-bucket/",
    "build-check": "NODE_ENV=development nuxt build --analyze",
    "generate": "nuxt generate",
    "clean": "rimraf .nuxt && rimraf ./dist",
    "start": "NODE_ENV=production node ./server.js",
    "static": "NODE_ENV=production node ./static.js",
    "static-test": "NODE_ENV=test node ./static.js",
    "sleep": "node ./scripts/sleep.js",
    "gen-imgs": "node ./scripts/responsive-imgs.js",
    "lint": "eslint --fix **/*.js *.js **/*.vue  --ignore-pattern node_modules --ignore-pattern dist --ignore-pattern .nuxt",
    "test-i": "BROWSER=chrome mocha --require babel-core/register ./test/integration/global.spec.js --timeout 30000 && BROWSER=safari mocha --require babel-core/register ./test/integration/global.spec.js --timeout 30000",
    "test-safari": "BROWSER=safari mocha --require babel-core/register ./test/integration/global.spec.js --timeout 30000",
    "test-u": "mocha --require babel-core/register ./test/unit/global.spec.js --timeout 30000",
    "test": "NODE_ENV=test npm run test-i && NODE_ENV=test npm run test-u",
    "deploy-prod": "./scripts/check-is-master.sh && NODE_ENV=production npm run build && ./scripts/deploy-prod.sh"
  },
  "main": "dist/app.bundle.js"
}
